  <groups>
    <!-- group to build hudson plugins-->
    <group name="all" pattern="${job}">
      <include>
        <job>plugin_blacklist</job>
        <job>plugin_clover</job>
        <job>plugin_cobertura</job>
        <job>plugin_console</job>
        <job>plugin_crowd2</job>
        <job>plugin_cvs</job>
        <job>plugin_dependency-viewer</job>
        <job>plugin_deploy</job>
        <job>plugin_dimensionsscm</job>
        <job>plugin_disk-usage_eclipse</job>
        <job>plugin_displayname</job>
        <job>plugin_email-ext</job>
        <job>plugin_git</job>
        <job>plugin_jcaptcha</job>
        <job>plugin_jfreechart</job>
        <job>plugin_jna-native_eclipse</job>
        <job>plugin_jumble</job>
        <job>plugin_label-column-view</job>
        <job>plugin_liverebel-deploy</job>
        <job>plugin_maven-repository-server</job>
        <job>plugin_mks-integrity</job>
        <job>plugin_multiline-tabbar</job>
        <job>plugin_surround-scm</job>
        <job>plugin_ui-samples</job>
        <job>plugin_upstream-downstream-view</job>
        <job>plugin_vault</job>
        <job>plugin_weblogic-deployer</job>
        <job>plugin_windows-slaves</job>
        <job>plugin_wrapper-script</job>
 	<job>hudson_core</job>
 	<job>hudson_core_eclipse</job>
      </include>     	      
    </group>
  </groups>
  <jobs>
    <!-- job for standard plugins -->
    <job name="standard-build" template="build_standard_plugin.ftl">
      <propertyset>
        <property name="history.build.days">-1</property>
        <property name="history.build.number">10</property>
        <property name="history.artifact.days">-1</property>
        <property name="history.artifact.number">-1</property>
        <property name="scm.trigger">*/5 * * * *</property>
        <property name="jdk">oracle_jdk_6</property>
      </propertyset> 
    </job>

    <job name="standard-plugin">
      <inherit>
        <job>standard-build</job>
      </inherit>
      <propertyset>
        <property name="mailer.recipients">henrik@hlyh.dk</property>
	<property name="mailer.notalways">false</property>
	<property name="mailer.breakers">true</property>
      </propertyset> 
    </job>

    <job name="github">
      <propertyset>
        <property name="scm.type">git</property>
        <property name="scm.git.host">github.com</property>
        <property name="scm.git.scheme">git</property>
        <property name="scm.git.tool">Default</property>
        <property name="scm.git.branch">master</property>
      </propertyset> 
    </job>
    <job name="eclipse">
      <propertyset>
        <property name="scm.type">git</property>
        <property name="scm.git.host">git.eclipse.org</property>
        <property name="scm.git.scheme">git</property>
        <property name="scm.git.tool">Default</property>
        <property name="scm.git.branch">master</property>
      </propertyset> 
    </job>

    <job  name="plugin_blacklist" >
      <inherit>
        <job>standard-plugin</job>
        <job>github</job>
      </inherit>
      <propertyset>
        <property name="displayname">Plugin: Blacklist</property>
        <property name="scm.git.path">/hudson-plugins/blacklist-plugin.git</property>
      </propertyset>
    </job>

