  <jobs>
    <!-- job for standard plugins -->
    <job name="standard-build" template="build_standard_plugin.ftl">
      <propertyset>
        <property name="history.build.days">-1</property>
        <property name="history.build.number">10</property>
        <property name="history.artifact.days">-1</property>
        <property name="history.artifact.number">-1</property>
        <property name="scm.trigger">*/5 * * * *</property>
        <property name="jdk">oracle_jdk_6</property>
        <property name="mailer.recipients">henrik@hlyh.dk</property>
	<property name="mailer.notalways">false</property>
	<property name="mailer.breakers">true</property>
      </propertyset> 
    </job>
    <job name="github">
      <propertyset>
        <property name="scm.type">git</property>
        <property name="scm.git.host">github.com</property>
        <property name="scm.git.scheme">git</property>
        <property name="scm.git.tool">Default</property>
        <property name="scm.git.branch">master</property>
      </propertyset> 
    </job>
    <job name="eclipse-git">
      <propertyset>
        <property name="scm.type">git</property>
        <property name="scm.git.host">git.eclipse.org</property>
